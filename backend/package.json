{
  "name": "url-shortener",
  "version": "1.0.0",
  "description": "URL Shortener service built with Node.js, Express, and MongoDB",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "url-shortener",
    "express",
    "mongodb",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "fast-check": "^3.13.2",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "semver": "^7.7.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
